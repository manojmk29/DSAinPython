def maxSubArray(nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        total=0
        for i in range(0,len(nums)):
            t=0
            for j in range(i,len(nums)):
                t+=nums[j]
                total=max(t,total)
        return total
a=[1,2,3,0,1,5,19,3,-7]
print(maxSubArray(a))