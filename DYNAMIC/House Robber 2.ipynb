    "def func(arr):\n",
    "    n=len(arr)\n",
    "    def helper(pos,tot):\n",
    "        if(pos>=n):\n",
    "            return(helper(pos%n,tot))\n",
    "        l=helper(pos+1,tot)\n",
    "        r=helper(pos+2,tot+arr[pos])\n",
    "        return(max(l,r))\n",
    "    return(helper(0,0))\n",
    "arr=[5, 5, 10, 100, 10, 5]\n",
    "func(arr)"
   ]
