   "source": [
    "from heapq import heappop,heapify,heappush\n",
    "a=[2,3,4,8,1,20]\n",
    "# heapq is minheap\n",
    "for i in range(len(a)):\n",
    "    a[i]=(-1*a[i])\n",
    "heapify(a)\n",
    "print(-1*heappop(a))\n",
    "heappush(a,-1*99)\n",
    "heappush(a,-1*123)\n",
    "print(-1*heappop(a))\n",
    "print(-1*a[0])"
   ]
