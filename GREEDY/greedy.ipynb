    "def coinsum(arr,val):\n",
    "    arr.sort(reverse=True)\n",
    "    count=0\n",
    "    for i in range(len(arr)):\n",
    "        if(arr[i]<=val):\n",
    "            count+=val//arr[i]\n",
    "            val=val%arr[i]\n",
    "    return(count)\n",
    "arr=[10,5,50,1,2]\n",
    "v=123\n",
    "print(coinsum(arr,v))\n"
   ]
